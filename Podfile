
platform :ios, ‘8.0’

use_frameworks!
#Framework

# ruby语法
# target数组 如果有新的target直接加入该数组
targetsArray = [‘FJChatMessageViewDemo’]
# 循环
targetsArray.each do |t|
    target t do


# ----------------------------------   View  ----------------------------------
	pod 'YYKit', '~> 1.0.9’
	pod 'FJPictureBrowser'
	pod 'Masonry'

    end
end

